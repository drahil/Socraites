{
    "name": "drahil/socraites",
    "autoload": {
        "psr-4": {
            "drahil\\Socraites\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "src/Helpers/socraites_configs.php"
        ]
    },
    "bin": [
        "bin/socraites"
    ],
    "authors": [
        {
            "name": "Omar Djecevic",
            "email": "djecevic.omar@gmail.com"
        }
    ],
    "require": {
        "nikic/php-parser": "^5.4",
        "symfony/console": "^7.2",
        "symfony/process": "^7.2",
      "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "pestphp/pest": "^3.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "drahil\\Socraites\\Providers\\SocraitesServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
