#!/usr/bin/env php
<?php

$autoloaders = [
    __DIR__.'/../vendor/autoload.php',
    __DIR__.'/../../../../autoload.php'
];

foreach ($autoloaders as $file) {
    if (file_exists($file)) {
        require $file;
        break;
    }
}

use drahil\Socraites\Console\Application;

$application = new Application();
$application->run();
